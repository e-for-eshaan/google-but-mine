{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "react/self-closing-comp": "warn",
    "@next/next/no-img-element": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}